<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>items</key>
	<array>
		<dict>
			<key>cell</key><string>PSGroupCell</string>
			<key>label</key><string>Auto Finish</string>
            <key>footerText</key><string>Auto Finish automatically closes or resprings the Cydia install screen without user input.</string>
		</dict>
		<dict>
			<key>cell</key><string>PSSwitchCell</string>
			<key>default</key><false/>
			<key>defaults</key><string>com.brycedev.nella</string>
			<key>key</key><string>kAutoFinish</string>
			<key>label</key><string>Enabled</string>
			<key>PostNotification</key><string>com.brycedev.nella/prefschanged</string>
		</dict>
        <dict>
            <key>cell</key><string>PSStaticTextCell</string>
            <key>label</key><string>Delay Time</string>
        </dict>
        <dict>
            <key>cell</key><string>PSSliderCell</string>
            <key>default</key><real>5.0</real>
            <key>defaults</key><string>com.brycedev.nella</string>
            <key>key</key><string>kFinishDelay</string>
            <key>isContinuous</key><false/>
            <key>max</key><real>15.0</real>
            <key>min</key><real>0.0</real>
            <key>showValue</key><true/>
            <key>PostNotification</key><string>com.brycedev.nella/prefschanged</string>
        </dict>
	</array>
	<key>title</key>
	<string>Misc. Options</string>
</dict>
</plist>
