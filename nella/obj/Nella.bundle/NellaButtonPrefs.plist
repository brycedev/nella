<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>items</key>
	<array>
		<dict>
			<key>cell</key><string>PSGroupCell</string>
			<key>label</key><string>Background Image</string>
            <key>footerText</key><string>Use an image as the background.</string>
		</dict>
		<dict>
			<key>cell</key><string>PSSwitchCell</string>
			<key>default</key><false/>
			<key>defaults</key><string>com.brycedev.nella</string>
			<key>key</key><string>kDisableBorder</string>
			<key>label</key><string>Disable Border</string>
			<key>PostNotification</key><string>com.brycedev.nella/prefschanged</string>
		</dict>
        <dict>
			<key>cell</key><string>PSGroupCell</string>
			<key>label</key><string>Border Radius</string>
            <key>footerText</key><string>Set the border radius.</string>
		</dict>
		<dict>
			<key>cell</key><string>PSStaticTextCell</string>
			<key>label</key><string>Border Radius</string>
		</dict>
		<dict>
			<key>cell</key><string>PSSliderCell</string>
			<key>default</key><real>10.0</real>
			<key>defaults</key><string>com.brycedev.nella</string>
			<key>key</key><string>kBorderRadius</string>
			<key>isContinuous</key><false/>
			<key>max</key><real>20.0</real>
			<key>min</key><real>0.0</real>
			<key>showValue</key><true/>
			<key>PostNotification</key><string>com.brycedev.nella/prefschanged</string>
		</dict>
        <dict>
			<key>cell</key><string>PSGroupCell</string>
			<key>label</key><string>Border Color</string>
            <key>footerText</key><string>Set the border color.</string>
		</dict>
		<dict>
            <key>cell</key><string>PSLinkCell</string>
            <key>cellClass</key><string>PFColorCell</string>
            <key>label</key><string>Border Color</string>
            <key>color_defaults</key><string>com.brycedev.nella</string>
            <key>color_key</key><string>kBorderColor</string>
            <key>title</key><string>Button Border Color</string>
            <key>color_fallback</key><string>#fff</string>
            <key>usesRGB</key><true/>
            <key>usesAlpha</key><false/>
            <key>color_postNotification</key><string>com.brycedev.nella/prefschanged</string>
        </dict>
		<dict>
			<key>cell</key><string>PSGroupCell</string>
			<key>label</key><string>Button Text Color</string>
            <key>footerText</key><string>Set the button text color.</string>
		</dict>
        <dict>
            <key>cell</key><string>PSLinkCell</string>
            <key>cellClass</key><string>PFColorCell</string>
            <key>label</key><string>Text Color</string>
            <key>color_defaults</key><string>com.brycedev.nella</string>
            <key>color_key</key><string>kButtonTextColor</string>
            <key>title</key><string>Button Text Color</string>
            <key>color_fallback</key><string>#fff</string>
            <key>usesRGB</key><true/>
            <key>usesAlpha</key><false/>
            <key>color_postNotification</key><string>com.brycedev.nella/prefschanged</string>
        </dict>
	</array>
	<key>title</key>
	<string>Button Options</string>
</dict>
</plist>
